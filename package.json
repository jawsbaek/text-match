{
  "name": "tanstarter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check": "bun run format && bun run lint && bun run check-types",
    "test": "vitest",
    "db": "drizzle-kit",
    "deps": "bunx taze@latest -Ilw",
    "deps:major": "bunx taze@latest major -Ilw",
    "ui": "bunx shadcn@latest",
    "auth:secret": "bunx @better-auth/cli@latest secret",
    "auth:generate": "bunx @better-auth/cli@latest generate --config ./src/lib/auth/auth.ts --y --output ./src/lib/db/schema/auth.schema.ts && prettier --write ./src/lib/db/schema/auth.schema.ts",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-devtools": "^0.6.4",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@tanstack/react-router": "^1.131.35",
    "@tanstack/react-router-devtools": "^1.131.35",
    "@tanstack/react-router-ssr-query": "^1.131.35",
    "@tanstack/react-start": "^1.131.35",
    "@vitejs/plugin-react": "^5.0.2",
    "better-auth": "^1.3.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "jose": "^5.9.6",
    "lucide-react": "^0.542.0",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.1.4",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.53.0",
    "@eslint/js": "^9.35.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/devtools-vite": "^0.2.12",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@tanstack/eslint-plugin-router": "^1.131.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "lefthook": "^1.12.4",
    "msw": "^2.6.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.4"
  }
}
